<div class="bg-info p-5">
        <h1 class="display-3 text-center">Want to advertize your products??</h1>
        <h1 class="mt-4 text-white"> Fill in the form below, our marketting manager will contact you shortly</h1>
        
        <div class="row mt-5">
                <div class="col-md-6 m-auto">
                  <div class="card card-body">
                    <h1 class="text-center mb-3">
                      <i class="fas fa-address-book"></i> Fill in
                    </h1>
                    
                    <form action="/users/register" method="POST">
                      <div class="form-group">
                        <label for="name">Name</label>
                        <input
                          type="name"
                          id="name"
                          name="name"
                          class="form-control"
                          placeholder="Enter Name"
                          value="<%= typeof name != 'undefined' ? name : '' %>"
                        />
                      </div>
                      <div class="form-group">
                        <label for="email">Email</label>
                        <input
                          type="email"
                          id="email"
                          name="email"
                          class="form-control"
                          placeholder="Enter Email"
                          value="<%= typeof email != 'undefined' ? email : '' %>"
                        />
                      </div>
                      <div class="form-group">
                        <label for="contactNo">Contact Number</label>
                        <input
                          type="number"
                          id="contactNo"
                          name="contactNumber"
                          class="form-control"
                          placeholder="Enter your number"
                          value="<%= typeof password != 'undefined' ? password : '' %>"
                        />
                      </div>
                      
                      <button type="submit" class="btn btn-primary btn-block">
                        Submit
                      </button>
                    </form>
                  </div>
                </div>
              </div>
</div>

